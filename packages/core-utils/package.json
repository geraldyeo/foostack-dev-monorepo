{
  "name": "@foostack-dev/core-utils",
  "version": "0.0.0",
  "description": "Core-utils module",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:geraldyeo/foostack-dev-monorepo.git"
  },
  "author": "Gerald Yeo <gerald.yeo@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.15.0",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check --watch",
    "prebuild": "rimraf ./lib",
    "build": "yarn run build:types && yarn run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "eslint --cache --cache-location \"../../node_modules/.eslintcache/\" .",
    "test": "jest --passWithNoTests",
    "test-coverage": "yarn run test --coverage"
  },
  "dependencies": {
    "invariant": "2.2.4",
    "lodash.get": "4.4.2",
    "lodash.identity": "3.0.0",
    "lodash.isnumber": "3.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.4",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-typescript": "7.3.3",
    "@types/invariant": "2.2.30",
    "@types/lodash.get": "4.4.6",
    "@types/lodash.identity": "3.0.6",
    "@types/lodash.isnumber": "3.0.6",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "eslint": "6.0.1",
    "eslint-plugin-jest": "22.10.0",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "typescript": "3.5.3"
  },
  "jest": {
    "coverageDirectory": "../../coverage"
  }
}
