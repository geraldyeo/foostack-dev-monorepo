{
  "name": "@foostack-dev/core",
  "version": "0.0.0",
  "description": "Core module",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:geraldyeo/foostack-dev-monorepo.git"
  },
  "author": "Gerald Yeo <gerald.yeo@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.0.0",
    "yarn": ">= 1.13.0"
  },
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "yarn run build:types && yarn run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel --root-mode upward src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "develop": "yarn run develop:js",
    "develop:types": "tsc --noEmit --watch",
    "develop:js": "yarn run build --watch"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "lodash.get": "^4.4.2",
    "lodash.identity": "^3.0.0",
    "lodash.isnumber": "^3.0.3"
  }
}
