{
  "name": "@foostack-dev/monorepo",
  "description": "foostack.dev monorepo",
  "repository": {
    "type": "git",
    "url": "git@github.com:geraldyeo/foostack-dev-monorepo.git"
  },
  "author": "Gerald Yeo <gerald.yeo@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.15.0",
    "yarn": ">=1.16.0"
  },
  "workspaces": [
    "clients/*",
    "packages/*",
    "testinggrounds/*",
    "servers/*"
  ],
  "scripts": {
    "prerelease": "yarn run build",
    "release": "cross-env HUSKY_BYPASS=true lerna publish",
    "release:dryrun": "yarn run release --no-git-tag-version --no-push",
    "build": "yarn run build:packages && yarn run build:gatsby",
    "build:docs": "yarn run build:packages && yarn run build:storybooks",
    "build:packages": "lerna run build --parallel --scope \"@foostack-dev/{core,restyled}\"",
    "build:gatsby": "lerna run build --stream --scope \"@foostack-dev/site\"",
    "build:storybooks": "lerna run build --stream --scope \"@foostack-dev/storybooks\"",
    "dev": "concurrently -k -r \"yarn:dev:packages\" \"yarn:dev:gatsby\"",
    "dev:docs": "concurrently -k -r \"yarn:dev:packages\" \"yarn:dev:storybooks\"",
    "dev:packages": "lerna run develop --parallel --scope \"@foostack-dev/{core,restyled}\"",
    "dev:gatsby": "yarn run waiton:packages && lerna run develop --stream --scope \"@foostack-dev/site\"",
    "dev:storybooks": "yarn run waiton:packages && lerna run develop --stream --scope \"@foostack-dev/storybooks\"",
    "lint": "yarn run lint:js && yarn run lint:ts",
    "lint:clients": "eslint \"clients/**/*.js\" --cache --cache-location \"./node_modules/.cache/\"",
    "lint:ts": "eslint \"packages/core/src/**/*.ts\" --cache --cache-location \"./node_modules/.cache/\"",
    "lint:js": "eslint \"packages/restyled/src/**/*.js\" --cache --cache-location \"./node_modules/.cache/\"",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests",
    "test:coverage": "yarn run test --coverage",
    "test:codacy-coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "waiton:packages": "wait-on -v file:packages/core/lib/index.js file:packages/restyled/lib/index.js"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-do-expressions": "7.2.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.3.2",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@babel/runtime": "7.4.5",
    "@babel/types": "7.4.4",
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-lerna-scopes": "8.0.0",
    "@commitlint/prompt-cli": "8.0.0",
    "@types/invariant": "2.2.30",
    "@types/lodash.get": "4.4.6",
    "@types/lodash.identity": "3.0.6",
    "@types/lodash.isnumber": "3.0.6",
    "@types/lodash.set": "4.3.6",
    "@types/styled-components": "4.1.16",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-macros": "2.6.1",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-gatsby": "0.2.0",
    "codacy-coverage": "3.4.0",
    "concurrently": "4.1.1",
    "cross-env": "5.2.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.7.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "jest-circus": "24.8.0",
    "jest-dom": "3.5.0",
    "jest-environment-jsdom": "24.8.0",
    "jest-environment-jsdom-global": "1.2.0",
    "jest-styled-components": "6.3.3",
    "lerna": "3.15.0",
    "lint-staged": "9.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "prop-types": "15.7.2",
    "react-hooks-testing-library": "0.6.0",
    "react-testing-library": "8.0.1",
    "rimraf": "2.6.3",
    "typescript": "3.5.2",
    "wait-on": "3.2.0"
  }
}
